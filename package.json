{
  "name": "nasa-path-finder",
  "version": "1.0.0",
  "description": "An app to find the shortest path between points in a 3D model of ISS",
  "main": "electron.js",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "dev": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "start": "webpack-dashboard -- webpack-dev-server",
    "start:native": "electron .",
    "start:web": "http-server",
    "test": "NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lovetostrike/nasa-path-finder"
  },
  "license": "Unlicense",
  "homepage": "https://github.com/lovetostrike/nasa-path-finder#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "electron": "^1.7.8",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.3.0",
    "react": "15.6.1",
    "react-dom": "15.4.2",
    "react-dropzone": "^4.1.3",
    "sass-loader": "^6.0.2",
    "stats-js": "^1.0.0-alpha1",
    "stats.js": "^0.17.0",
    "three": "^0.87.1",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.22.0",
    "css-loader": "0.26.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^20.0.4",
    "loglevel": "^1.4.1",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^3.3.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js",
      "app": "<rootDir>/src/app.jsx"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
